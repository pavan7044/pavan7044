// swaping of two variables 

import java.util.*;

class Swap{

int x,y;

public void Swaping(){
Scanner sc= new Scanner(System.in);
System.out.println("enter x number");
x=sc.nextInt();
System.out.println("enter y number");
x=sc.nextInt();
System.out.println("Befora Swapping "+x+"    "+y);
int z=x;
x=y;
y=z;

System.out.println("Befora Swapping "+x+"    "+y);
}
public static void main(String args[]){
	Swap s1=new Swap();
	s1.Swapping();
}
}
